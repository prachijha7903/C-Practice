#include<stdio.h>
int main(){
    int i,n,p;
    printf("Enter the number :");
    scanf("%d",&n);

  for(i=2;i<n;i++){
       p=n%i;
       if(p==0)
       break;

  }
    if(n==1){
        printf("The number is neither prime nor composite");
    }
    else if(p==0){
        printf("The number is a composite number ");
    }
    else {
        printf("The number is a prime number");
    }
    return 0;
}
