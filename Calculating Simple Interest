#include<stdio.h>      //Simple interest
int main(){
    float p,r,t,SI;
    printf("Enter the principle: ");
    printf("Enter the rate of interest in %: ");
    printf("Enter the time: ");
    scanf("%f%f%f",&p,&r,&t);
    SI=(p*r*t)/100;
    printf("The Simple interest value is:%f",SI);
    return 0;
}
