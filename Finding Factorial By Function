#include<stdio.h>
int factorial(int x){
    int fact = 1;
    for(int i=2; i<=x ; i++){
        fact = fact*i;
    }
    return fact;
}
int main() {
    int n;
    printf("Enter the Number : ");
    scanf("%d",&n);
    int a = factorial(n);
    printf("The Factorial of given number is : %d", a);
    return 0;
}
