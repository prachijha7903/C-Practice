#include<stdio.h>
int factorial(int n){
    if(n==1 || n==0) return 1;
    int recAns = n*factorial(n-1);
    return recAns;                
}                           
int main(){
    int n;
    printf("enter a number : ");
    scanf("%d",&n);
    int fact = factorial(n);// factorial(anything) means 
    printf("%d",fact);// calling that function
    return 0;
}
