#include<stdio.h>
void namaste();
void bonjour();
int main() {
    char ch;
    printf("Enter i if indian else enter f if french :");
    scanf("%c",&ch);
    if(ch=='i'){
        namaste();
    }
    else //(ch=='f')
      { 
        bonjour();
    }
    return 0;
}
void namaste(){
    printf("Namaste");
}
void bonjour(){
    printf("Bonjour");
}
