#include<stdio.h>
void swap(int *a,int *b);

int main(){
    int a,b;
    a=5,b=8;
    printf("before swapping a=%d\n",a);
    printf("before swapping b=%d\n",b);

  swap(&a,&b);

  printf("after swapping a =%d\n",a);
    printf("after swapping b=%d\n",b);
    return 0;
}

void swap(int *a,int *b){
    int temp = *a;
    *a = *b;
    *b = temp;
}
