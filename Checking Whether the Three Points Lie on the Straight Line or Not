#include<stdio.h>
int main() {
    float x1 , y1 , x2 , y2 , x3 , y3 ;
    printf("Enter coordinate of x for point 1 : ");
    scanf("%f", &x1);
    printf("Enter coordinate of y for point 1 : ");
    scanf("%f" , &y1);
    printf("Enter coordinate of x for point 2 : ");
    scanf("%f" ,&x2);
    printf("Enter coordinate of y for point 2 : ");
    scanf("%f" , &y2);
    printf("Enter coordinate of x for point 3 : ");
    scanf("%f" , &x3);
    printf("Enter coordinate of y for point 3 : ");
    scanf("%f" , &y3);
    float m1 =(y2-y1)/(x2-x1);
    float m2 =(y3-y2)/(x3-x2);
    if(m1==m2){
        printf("These three points lie on straight line");
    }
    else{
        printf("These three points doesn't lie on a straight line");
    }
    return 0;
}
