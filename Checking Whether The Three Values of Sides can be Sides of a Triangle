#include<stdio.h>
int main () {
    int a,b,c;
    printf("Enter the three sides of triangle : ");
    scanf("%d %d %d", &a ,&b , &c);
    if((a+b)>c && (a+c)>b && (b+c)>a){
        printf("These are the three sides of triangle");
    }
    else { 
        printf("These are not the three sides of triangle");
    }

  return 0;
}
