#include<stdio.h>
#include<string.h>

struct student{
    char name[100];//char *name;
    int roll;
    float cgpa;
};

int main(){
    struct student s1;
    s1.roll = 10085;
    s1.cgpa = 9.4;
    strcpy(s1.name,"Krishna");

   printf("Student name =%s\n",s1.name);
    printf("Student roll:=%d\n",s1.roll);
    printf("Student cgpa = %2f\n",s1.cgpa);
    return 0;
}
