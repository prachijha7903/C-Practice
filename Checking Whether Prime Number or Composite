#include<stdio.h>
int main(){
    int n,p;
    printf("ENter the number :");
    scanf("%d",&n);
    p=0;
    for(int i=2;i<n;i++){
        if(n%i==0)
        p=1;
         break;
    }
    if(n==1) printf("The number %d is neither a prime number nor a composite number",n);
    else if(p==1) printf("The number %d is a Composite number ",n);
    else printf("The number %d is a Prime Number",n);
    return 0;
}
